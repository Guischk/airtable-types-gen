{
  "name": "airtable-types-gen",
  "author": "guischk",
  "version": "0.1.0",
  "description": "Generate TypeScript types from Airtable base schemas with optional record flattening",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "airtable-types-gen": "./bin/airtable-types-gen"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./runtime": {
      "import": "./dist/runtime/index.js",
      "require": "./dist/runtime/index.js",
      "types": "./dist/runtime/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:package": "./test-package.sh",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "airtable",
    "typescript",
    "types",
    "generator",
    "cli"
  ],
  "license": "MIT",
  "dependencies": {
    "airtable": "^0.12.2",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "vite": "^5.1.4",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/airtable-types-gen.git"
  }
}
